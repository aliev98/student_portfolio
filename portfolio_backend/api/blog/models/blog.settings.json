{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blog",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "html",
        "csharp",
        "Winforms",
        "Praktik",
        "Sass",
        "Rug",
        "RtyledComponents",
        "React",
        "Gatsby"
      ],
      "required": true
    },
    "desc": {
      "type": "text",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    }
  }
}
